{
    "manifest_version": 3,
    "name": "onchainrank DEX Plugin",
    "version": "1.51",
    "description": "Adds the advanced chart for trading meme coins on pump.fun and provides a settings GUI for entering an API key.",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "ocr16.png",
        "48": "ocr48.png",
        "128": "ocr128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "ocr16.png",
            "48": "ocr48.png",
            "128": "ocr128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://neo.bullx.io/terminal*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}